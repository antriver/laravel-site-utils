{
    "name": "antriver/laravel-site-utils",
    "description": "A bunch of stuff to help building Laravel apps.",
    "type": "library",
    "authors": [
        {
            "name": "Anthony Kuske",
            "homepage": "https://anthonykuske.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antriver\\LaravelSiteUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antriver\\LaravelSiteUtilsTests\\": "tests/",
            "Antriver\\LaravelSiteUtilsTestApp\\": "test-laravel-app/app/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "antriver/laravel-database-session-auth": "^6.0",
        "antriver/laravel-model-presenters": "^1.0",
        "antriver/laravel-registers": "^6.0",
        "antriver/laravel-repositories": "^10.0",
        "antriver/laravel-simple-message-trait": "^5.8",
        "aws/aws-php-sns-message-validator": "^1.6",
        "erusev/parsedown": "^1.7",
        "firebase/php-jwt": "^5.0",
        "google/recaptcha": "^1.2",
        "intervention/image": "^2.5",
        "laravel/framework": "^11.0",
        "laravel/socialite": "^5.12",
        "league/flysystem-aws-s3-v3": "^3.27",
        "tokenly/token-generator": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antriver\\LaravelSiteUtils\\Providers\\LaravelSiteUtilsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
